{
    "name": "tgkw-adc/helper",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "adc帮助类",
    "autoload": {
        "psr-4": {
            "TgkwAdc\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "firebase/php-jwt": "^6.11",
        "hyperf/amqp": "^3.1",
        "hyperf/cache": "~3.1.0",
        "hyperf/circuit-breaker": "*",
        "hyperf/command": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/config-center": "*",
        "hyperf/config-nacos": "*",
        "hyperf/constants": "^3.1",
        "hyperf/crontab": "*",
        "hyperf/database": "~3.1.0",
        "hyperf/db-connection": "^3.1",
        "hyperf/engine": "^2.10",
        "hyperf/filesystem": "*",
        "hyperf/flysystem-oss": "*",
        "hyperf/framework": "~3.1.0",
        "hyperf/guzzle": "^3.1",
        "hyperf/helper": "*",
        "hyperf/http-server": "~3.1.0",
        "hyperf/json-rpc": "*",
        "hyperf/logger": "~3.1.0",
        "hyperf/memory": "~3.1.0",
        "hyperf/model-cache": "^3.1",
        "hyperf/nacos": "^3.1",
        "hyperf/pool": "^3.1",
        "hyperf/process": "~3.1.0",
        "hyperf/redis": "^3.1",
        "hyperf/resource": "^3.1",
        "hyperf/rpc-client": "*",
        "hyperf/rpc-server": "*",
        "hyperf/service-governance": "*",
        "hyperf/service-governance-nacos": "*",
        "hyperf/snowflake": "^3.1",
        "hyperf/validation": "^3.1",
        "hyperf/xxl-job-incubator": "^0.7.0",
        "league/flysystem-aws-s3-v3": "3.0",
        "overtrue/flysystem-cos": "5.0",
        "mpdf/mpdf": "^8.2",
        "overtrue/flysystem-qiniu": "3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "dev-master"
    },
    "suggest": {
        "swow/swow": "Required to create swow components."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "TgkwAdc\\ConfigProvider"
        }
    },
    "repositories": [
        {
            "description": "华为Composer全量镜像",
            "type": "composer",
            "url": "https://repo.huaweicloud.com/repository/php/",
            "canonical": false
        },
        {
            "description": "官方源",
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    ]
}
