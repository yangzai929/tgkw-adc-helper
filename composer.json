{
    "name": "tgkw-adc/helper",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "adc帮助类",
    "autoload": {
        "psr-4": {
            "TgkwAdc\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^3.0",
        "donjan-deng/hyperf-casbin": "^3.23",
        "dtm/dtm-client": "^0.3.5",
        "firebase/php-jwt": "^6.11",
        "friendsofhyperf/mail": "^3.1",
        "hyperf/amqp": "^3.1",
        "hyperf/cache": "~3.1.0",
        "hyperf/circuit-breaker": "~3.1.0",
        "hyperf/command": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/config-center": "~3.1.0",
        "hyperf/config-nacos": "~3.1.0",
        "hyperf/constants": "^3.1",
        "hyperf/crontab": "~3.1.0",
        "hyperf/database": "~3.1.0",
        "hyperf/db-connection": "^3.1",
        "hyperf/engine": "^2.10",
        "hyperf/filesystem": "~3.1.0",
        "hyperf/flysystem-oss": "~1.4.0",
        "hyperf/framework": "~3.1.0",
        "hyperf/grpc": "3.1.*",
        "hyperf/grpc-client": "^3.1",
        "hyperf/guzzle": "^3.1",
        "hyperf/helper": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/http2-client": "3.1.*",
        "hyperf/json-rpc": "~3.1.0",
        "hyperf/logger": "~3.1.0",
        "hyperf/memory": "~3.1.0",
        "hyperf/model-cache": "^3.1",
        "hyperf/nacos": "^3.1",
        "hyperf/pool": "^3.1",
        "hyperf/process": "~3.1.0",
        "hyperf/rate-limit": "^3.1",
        "hyperf/redis": "^3.1",
        "hyperf/resource": "^3.1",
        "hyperf/rpc-client": "~3.1.0",
        "hyperf/rpc-server": "~3.1.0",
        "hyperf/service-governance": "~3.1.0",
        "hyperf/service-governance-nacos": "~3.1.0",
        "hyperf/snowflake": "^3.1",
        "hyperf/translation": "^3.1",
        "hyperf/validation": "^3.1",
        "hyperf/xxl-job-incubator": "^0.7.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mpdf/mpdf": "^8.2",
        "overtrue/flysystem-cos": "^5.0",
        "overtrue/flysystem-qiniu": "^3.0",
        "phpoffice/phpspreadsheet": "^5.2",
        "phpoffice/phpword": "*",
        "ramsey/uuid": "^4.9",
        "tgkw-adc/flysystem-huaweicloud-obs": "1.0.3",
        "tgkw-adc/hyperf-captcha": "^1.0",
        "tgkw-adc/ip2location-php": "^1.0",
        "viest/php-ext-xlswriter-ide-helper": "1.3.6",
        "zoujingli/ip2region": "3.0.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "dev-master"
    },
    "suggest": {
        "swow/swow": "Required to create swow components."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix --config=.php-cs-fixer.php",
        "cs-check": "php-cs-fixer fix --config=.php-cs-fixer.php --dry-run --diff"
    },
    "extra": {
        "hyperf": {
            "config": "TgkwAdc\\ConfigProvider"
        }
    },
    "repositories": [
        {
            "description": "清华源Composer镜像（优先，稳定快速）",
            "type": "composer",
            "url": "https://packagist.mirrors.sjtug.sjtu.edu.cn",
            "canonical": true
        },
        {
            "description": "华为Composer全量镜像",
            "type": "composer",
            "url": "https://repo.huaweicloud.com/repository/php/",
            "canonical": false
        },
        {
            "description": "官方源",
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    ]
}
